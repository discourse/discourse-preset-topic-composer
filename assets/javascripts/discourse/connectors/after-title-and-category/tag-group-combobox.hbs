{{#if currentUser}}

  {{#each this.tagGroupList as |tagGroupOption|}}
    <div>
      {{tagGroupOption.tagGroup}}:
    </div>
    {{#if tagGroupOption.multi }}
      {{tag-group-multiselect tagGroupOption=tagGroupOption composer=@outletArgs.model}}
    {{else}}
      {{tag-group-combobox tagGroupOption=tagGroupOption composer=@outletArgs.model}}
    {{/if}}
  {{/each}}

{{/if}}
